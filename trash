    local M87 = game.workspace.Bosses.M87Boss.Head 

M87:Destroy()
end)

Sectionfrom2ndtab:NewButton("Fell", "ButtonInfo", function()
    local fell = game.workspace.Bosses.UnderFellBoss.Head 

fell:Destroy()
end)
Sectionfrom2ndtab:NewButton("Fell", "ButtonInfo", function()
    local fell2 = game.workspace.Bosses.UnderFellBossPhase2.Head 

fell2:Destroy()
end)

local Thirdtab = Window:NewTab("Chance bosses")
local Thirdtabsection = Thirdtab:NewSection("Bosses")

Thirdtabsection:NewButton("Sans...?", "ButtonInfo", function()
    local SS = game.workspace.Bosses.Sans.Head 

SS:Destroy()
end)

Thirdtabsection:NewButton("Teleport to map", "Too lazy to create new cattegory", function()
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(90.7008438, 10.4600544, -1118.40674, -0.533847094, -8.60358966e-08, 0.845581055, 1.70362817e-08, 1, 1.12503308e-07, -0.845581055, 7.44651203e-08, -0.533847094)
