local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("I suck at coding, please send help", "DarkTheme")

--Main
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")

MainSection:NewButton("Corrupted", "ButtonInfo", function()
    local script = game.workspace.Bosses.CorruptedSans.Head 

script:Destroy()
end)

MainSection:NewButton("Killer", "ButtonInfo", function()
local killer = game.workspace.Bosses.KillerSans.Head 

killer:Destroy()
end)

MainSection:NewButton("Dust", "ButtonInfo", function()
local dust = game.workspace.Bosses.DustSans.Head 

dust:Destroy()
end)

MainSection:NewButton("Horror", "ButtonInfo", function()
local horror = game.workspace.Bosses.HorrorSans.Head 

horror:Destroy()
end)

MainSection:NewButton("MTT", "ButtonInfo", function()
local mtth = game.workspace.Bosses.MTTHorrorSans.Head 

mtth:Destroy()
wait(0.1)


local mttd = game.workspace.Bosses.MTTDustSans.Head 

mttd:Destroy()

wait(0.1)

local mttk = game.workspace.Bosses.MTTKillerSans.Head 

mttk:Destroy()
wait(0.1)
end)

MainSection:NewButton("Error", "ButtonInfo", function()
local err = game.workspace.Bosses.ErrorSans.Head 

err:Destroy()
end)

MainSection:NewButton("Hyper", "ButtonInfo", function()
local pieceofshit = game.workspace.Bosses.HyperDustSans.Head 

pieceofshit:Destroy()
end)

MainSection:NewButton("Gameender", "ButtonInfo", function()
local pieceofshit2 = game.workspace.Bosses.GAMEENDER.Head 

pieceofshit2:Destroy()
end)

MainSection:NewButton("TRUEGameender", "ButtonInfo", function()
local pieceofshit3 = game.workspace.Bosses.TRUEGAMEENDER.Head 

pieceofshit3:Destroy()
end)

--Secondtab
